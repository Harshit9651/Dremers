<!-- JavaScript for handling icon visibility -->
<script>
  let lastScrollTop = 0;
  let isScrollingUp = false;

  window.addEventListener('scroll', function() {
    let currentScroll = window.pageYOffset || document.documentElement.scrollTop;

    if (currentScroll > lastScrollTop) {
      // Scroll down
      isScrollingUp = false;
    } else {
      // Scroll up
      isScrollingUp = true;
    }

    lastScrollTop = currentScroll;

    // Toggle the visibility of the icons based on the scrolling direction
    toggleMobileIcons();
  }, false);

  function toggleMobileIcons() {
    let mobileIcons = document.getElementById('mobileBottomIcons');

    if (isScrollingUp) {
      // Scroll up, show the icons
      mobileIcons.style.display = 'flex';
    } else {
      // Scroll down, hide the icons
      mobileIcons.style.display = 'none';
    }
  }
</script>
<script>
  // Function to show the education loan modal after 5 seconds
  setTimeout(function() {
    $('#educationLoanModal').modal('show');
  }, 5000);
  $('.modal .close').on('click', function() {
      $(this).closest('.modal').modal('hide');
    });

    // Function to close the modal when the backdrop is clicked
    $('.modal').on('click', function(e) {
      if ($(e.target).hasClass('modal')) {
        $(this).modal('hide');
      }
    });
</script>


